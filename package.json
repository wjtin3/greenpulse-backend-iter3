{
  "name": "greenpulse-backend",
  "version": "1.0.0",
  "description": "Backend API for GreenPulse carbon footprint calculator",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node scripts/migrate.js",
    "import-csv": "node scripts/import-csv.js",
    "import-csv-fixed": "node scripts/import-csv-fixed.js",
    "clear-data": "node scripts/clear-data.js",
    "import-csv-quoted": "node scripts/import-csv-quoted.js",
    "fix-shopping": "node scripts/fix-shopping-data.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "setup-rag": "node scripts/setupRAGTables.js",
    "populate-embeddings": "node scripts/populateEmbeddings.js",
    "populate-emission-factors": "node scripts/populateEmissionFactors.js",
    "import-food-consumption": "node scripts/import-food-consumption.js",
    "download-gtfs": "node scripts/downloadGTFS.js",
    "setup-gtfs-realtime": "node scripts/setupGTFSRealtime.js",
    "test-gtfs-realtime": "node test/test-gtfs-realtime.js",
    "setup-routing": "node scripts/setupRoutingSchema.js",
    "test-routing": "node test/test-routing.js",
    "optimize-gtfs-indexes": "node scripts/optimizeGTFSIndexes.js",
    "setup-route-cache": "node scripts/setupRouteCache.js",
    "cache-popular-routes": "node scripts/cachePopularRoutes.js",
    "cache-transit-routes": "node scripts/cachePopularTransitRoutes.js",
    "import-ktmb": "node scripts/importKTMBData.js",
    "realtime-updater": "node scripts/realtimeUpdater.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:integration": "jest tests/integration/",
    "test:vercel": "node scripts/test-vercel.js"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "cohere-ai": "^7.18.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "groq-sdk": "^0.32.0",
    "gtfs-realtime-bindings": "^1.1.1",
    "helmet": "^7.1.0",
    "marked": "^16.2.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "unzipper": "^0.10.14"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@jest/globals": "^30.2.0",
    "babel-jest": "^30.2.0",
    "drizzle-kit": "^0.20.4",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4"
  }
}
